{
  "name": "semantic-test-khilkevych",
  "version": "1.2.4",
  "private": true,
  "dependencies": {
    "lerna": "2.0.0-beta.31",
    "lerna-semantic-release": "^9.1.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-scripts": "2.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "semantic-release": "semantic-release",
    "semantic-release-monorepo": "npx semantic-release -e semantic-release-monorepo",
    "lerna-semantic-release": "lerna exec --concurrency 1 -- npx --no-install semantic-release -e semantic-release-monorepo",
    "lerna-semantic": "lerna-semantic-release pre lerna-semantic-release perform"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "workspaces": {
    "packages": [
      "libs/*"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LotharII/semantic-test.git"
  },
  "devDependencies": {
    "semantic-release": "15.10.3",
    "semantic-release-monorepo": "6.1.1"
  },
  "monorepo": {
    "mode": "locked",
    "packages": [
      "libs/khilkevych-components",
      "libs/khilkevych-core"
    ]
  }
}
